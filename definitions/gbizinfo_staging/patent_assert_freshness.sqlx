config {
  type: "assertion",
  schema: "gbizinfo_staging",
  tags: ["daily", "gbizinfo"]
}

WITH freshness AS (
  SELECT
    CURRENT_DATE("Asia/Tokyo") - MAX(application_date) AS delay
  FROM
    ${ref("gbizinfo_staging", "patent_stg")} )

SELECT
  *
FROM
  freshness
WHERE
  delay >= INTERVAL 364 DAY
