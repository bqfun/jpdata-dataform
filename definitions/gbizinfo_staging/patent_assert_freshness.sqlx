config {
  type: "assertion",
  schema: "gbizinfo_staging",
  tags: ["daily"]
}

SELECT
  COUNT(*) AS num_rows,
  CURRENT_DATE("Asia/Tokyo") - MAX(application_date) AS delay,
FROM
  ${ref("gbizinfo_staging", "patent_stg")}
HAVING
  num_rows < 4370781 -- 2022-11-13時点の行数
  OR INTERVAL 319 DAY < delay
