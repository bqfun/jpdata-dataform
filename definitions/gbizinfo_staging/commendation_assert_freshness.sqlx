config {
  type: "assertion",
  schema: "gbizinfo_staging",
  tags: ["daily"]
}

SELECT
  COUNT(*) AS num_rows,
  CURRENT_DATE("Asia/Tokyo") - MAX(date_of_commendation) AS delay,
FROM
  ${ref("gbizinfo_staging", "commendation_stg")}
HAVING
  num_rows < 63385 -- 2022-11-13時点の行数
  OR INTERVAL 291 DAY < delay
