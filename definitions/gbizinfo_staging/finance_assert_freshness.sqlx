config {
  type: "assertion",
  schema: "gbizinfo_staging",
  tags: ["daily"]
}

WITH freshness AS (
  SELECT
    CURRENT_DATE("Asia/Tokyo") - MAX(fiscal_year_cover_page.end_date) AS delay
  FROM
    ${ref("gbizinfo_staging", "finance_stg")} )

SELECT
  *
FROM
  freshness
WHERE
  delay >= INTERVAL 102 DAY
