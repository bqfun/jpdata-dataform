config {
  type: "assertion",
  schema: "houjinbangou_staging",
  tags: ["monthly"]
}

SELECT
  COUNT(*) AS num_rows,
  CURRENT_DATE("Asia/Tokyo") - MAX(update_date) AS delay,
FROM
  ${ref("houjinbangou_staging", "latest_stg")}
WHERE
  num_rows < 5231297 -- 2022-11-13時点の行数
  OR INTERVAL 32 DAY < delay
