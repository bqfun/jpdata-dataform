config {
  type: "assertion",
  schema: "houjinbangou_staging",
  disabled: true,
  tags: ["monthly"]
}

WITH freshness AS (
  SELECT
    CURRENT_DATE("Asia/Tokyo") - MAX(update_date) AS delay
  FROM
    ${ref("houjinbangou_staging", "change_history_stg")} )

SELECT
  *
FROM
  freshness
WHERE
  delay > INTERVAL 32 DAY
